{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CORS": {
    "AllowedOrigins": [ "https://some-front-end.com" ],
    "AllowedHeaders": [ "Authorization", "Content-Type" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
    "PreflightMaxAge": 86400
  },

  "ApiSettings": {
    "JwtOptions": {
      "Secret": "sekretkeysekretkeysekretkey",
      "Issuer": "auth-api",
      "Audience": "client"
    }
  }
}
